<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:fluent="https://github.com/DmitryNizhebovsky/Avalonia.RangeSlider"
             x:Class="MSpecpp.App"
             RequestedThemeVariant="Default">
             <!-- "Default" ThemeVariant follows system theme variant. "Dark" or "Light" are other available options. -->
             
             <Application.Resources>
	             <ResourceDictionary>
		             <ResourceDictionary.MergedDictionaries>
			             <ResourceInclude Source="/Icons.axaml"/>
		             </ResourceDictionary.MergedDictionaries>
	             </ResourceDictionary>
             </Application.Resources>
             
    <Application.Styles>
		    <fluent:RangeSliderTheme />
	    <FluentTheme>
			<FluentTheme.Palettes>
				<!-- Palette for Light theme variant -->
				<ColorPaletteResources x:Key="Light" />
				<!-- Palette for Dark theme variant -->
				<ColorPaletteResources x:Key="Dark" RegionColor="#161616" />
			</FluentTheme.Palettes>
		</FluentTheme>
	    <StyleInclude Source="/Styles.axaml"/>
    </Application.Styles>
</Application>
